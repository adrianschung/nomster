<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">
    <h1>
      <%= place.name %>
    </h1>
    <i><%= place.address %></i><br /><br />
    <p>
      <%= place.description %>
    </p>
  </div>
<% end %>

<div class="digg_pagination offset-1">
  <%= will_paginate @place %>
</div>